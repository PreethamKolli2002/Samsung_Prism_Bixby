macro-def (API_Events) {
  params {
    param (event) {
      type (EventOutput)
      min (Required) max (One)
    }
  }
  content {
    compound-card {
      content {
        image-card {
          aspect-ratio (4:3)
          title-area {
            halign (Start)
            slot1 {
              text {
                value ("")
              }
            }
          }
          image-url ("[#{value(event.imgUrl)}]")
        }
        paragraph {
          value ("#{raw(event.title)}")
          style (Title_S)
        }
        // single-line {
        //   text {
        //     value ("From #{value(hotel.lowRate)} â€¢ #{value(hotel.rating)} stars")
        //     style (Detail_L_Soft)
        //   }
        // }
      }
      on-click {
        view-for (event)
      }
    }
  }
}